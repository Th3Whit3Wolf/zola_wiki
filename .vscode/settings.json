{
    "liveSassCompile.settings.formats":[
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "/static/css"
        },
        {
            "format": "compressed",
            "extensionName": ".min.css",
            "savePath": "/static/css"
        }
    ],
    "liveSassCompile.settings.excludeList": [ 
        "**/node_modules/**",
        ".vscode/**" ,
        "**/sass/bulma/**",
        "**/static/fonts/Fira/**"
    ],
    "liveSassCompile.settings.generateMap": false,
    "liveSassCompile.settings.autoprefix": [
        "> 1%",
        "last 2 versions"
    ],
    "markdownlint.ignore": [
        "MD033",
        "MD025"
    ],
    "git.ignoreLimitWarning": true
}